<ncs-package xmlns="http://tail-f.com/ns/ncs-packages">
  <name>router</name>
  <package-version>1.0</package-version>
  <description>Model driven CLI NED using the SSHClient API</description>
  <ncs-min-version>6.0</ncs-min-version>
  <component>
    <name>router-cli</name>
    <ned>
      <cli>
        <ned-id>replaced by build</ned-id>
        <java-class-name>
          com.tailf.packages.ned.routercli.RouterCli
        </java-class-name>
      </cli>
      <device>
        <vendor>Tail-f</vendor>
        <product-family>Tail-f CLI router 1.0</product-family>
        <product-family>Tail-f CLI router 2.0</product-family>
        <operating-system>CLI 1+</operating-system>
        <operating-system>CLI</operating-system>
      </device>
    </ned>
  </component>
</ncs-package>
